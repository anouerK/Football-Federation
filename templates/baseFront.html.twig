{% block body %}
<!--MAIN MACTH SHEDULE BEGIN-->

    <section class="main-match-shedule">
        <div class="container">
            <div class="row">
                <div class="col-md-6 col-sm-12 col-xs-12">
                    <h6>Last match</h6>
                    {% set rows = 1 %}
                    {% for i in z %}

                    {% if rows <= 1 %}
                    {% set rows = rows +1 %}
                    <div class="main-next-match bg-cover">
                        <img src="{{ asset('uploads/' ~ i.stade.photo) }}" class="next-match-background-img" alt="next-image">
                        <div class="wrap">
                            <div class="place" >{{ i.stade }}</div>
                            <div class="date" >{{ i.dated |date('F j-Y-g:i a')}}</div>
                            <div class="teams-wrap" >
                                <a href="{{ path('detailCl', { 'id': i.club1.id }) }}" class="team">
                                    <span>{{ i.club1 }}</span>
                                    <span><img src="{{ asset('uploads/' ~ i.club1.logo) }}" alt="team-image" ></span>
                                </a>
                                <div class="vs">
                                    {{ i.r1 }}:{{ i.r2 }}
                                </div>
                                <a href="{{ path('detailCl', { 'id': i.club2.id }) }}" class="team1">
                                    <span><img  src="{{ asset('uploads/' ~ i.club2.logo) }}" alt="team-image"></span>
                                    <span>{{ i.club2 }}</span>
                                </a>
                            </div>
                            <a href="{{ path('detailG', { 'id': i.id }) }}" class="booking"> MATCH</a>
                        </div>
                    </div>
                </div>
                {% endif %}
                {% endfor %}


                <div class="col-md-6 col-sm-12 col-xs-12">
                    <h6>Latest matches</h6>

                    <div class="main-lates-matches">
                        {% set rows = 0 %}
                        {% for i in z %}
                            {% set rows = rows +1 %}
                            {% if rows < 6 %}
                                <a href="{{ path('detailG', { 'id': i.id }) }}" class="item">
                                    <span class="championship">{{ i.tournoi }} - {{ i.stade }}</span>
                                    <span class="teams-wrap">
                    <span class="team">
                        <span>
                        <img src="{{ asset('uploads/' ~ i.club1.logo) }}" alt="" style="width: 40px" style="height:40px">
                        </span>
                        <span>{{ i.club1 }}</span>
                    </span>
                    <span class="score">
                        <span>{{ i.r1 }}:{{ i.r2 }}</span>

                    </span>
                    <span class="team1">
                        <span>{{ i.club2 }}</span>
                        <span><img src="{{ asset('uploads/' ~ i.club2.logo) }}" alt="" style="width: 40px" style="height:40px"></span>
                    </span>
            </span>
                                    <span class="game-result">{{ i.dated |date('F j-Y-g:i a')}} - Arbitre:{{ i.arbitre}}</span>
                                </a>
                            {% endif %}
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!--MAIN MACTH SHEDULE END-->

    <!--MAIN PLAYERS STAT BEGIN-->

    <section class="main-players-stat bg-cover">
        <div class="container">
            <div class="row">
                <div class="col-md-7">
                    <h4 class="hockey-stats-h4">Players stats</h4>


                    <div class="single-player-stats players_statistic_slider">

                        <div class="player-stat-slider tab-content">
                            <div id="slider" class="carousel slide" data-ride="carousel">
                                <div class="carousel-inner tab-content" role="listbox">

                                    <div class="item active tab-pane" id="deffender" >

                                        <div class="wrap">
                                            <div class="stat">
                                                <div class="top-info with_number">
                                                    <div ><img src="images/soccer/fede.png" alt="" width="70px"></div>



                                                </div>
                                                <h1> Best Players </h1>

                                            </div>
                                            <div class="image">

                                                <img src="images/soccer/news3.jpg" alt="player-image">

                                            </div>
                                        </div>
                                    </div>
                                    {% set rows = 0 %}
                                    {% for player in j  %}
                                        {% set rows = rows +1 %}
                                        {% if rows < 7 %}
                                            <div class="item  tab-pane" id="deffender" >

                                                <div class="wrap">
                                                    <div class="stat">
                                                        <div class="top-info with_number">
                                                            <div class="number">{{player.numt}}</div>
                                                            <a href="{{ path('detailJ', { 'cin': player.cin }) }}" class="name">
                                                                {{player.nom }} {{ player.prenom }}
                                                            </a>
                                                        </div>
                                                        <div class="position">
                                                            {{player.poste }} - {{player.club.nomc}}
                                                        </div>
                                                        <div class="progress-wrap">
                                                            <div class="progress-item">
                                                                <div class="bar-label">
                                                                    <div class="achievement">Scores </div>
                                                                    <div class="score">{{player.nbm}}</div>
                                                                </div>
                                                                <div class="progress-line">
                                                                    <div class="bar bar-1" ></div>
                                                                </div>
                                                            </div>
                                                            <div class="progress-item">
                                                                <div class="bar-label">
                                                                    <div class="achievement">Assists</div>
                                                                    <div class="score">{{player.nba}}</div>
                                                                </div>
                                                                <div class="progress-line">
                                                                    <div class="bar bar-2" ></div>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>
                                                    <div class="image">
                                                        <a href="{{ path('detailJ', { 'cin': player.cin }) }}" title="{{player.nom }} {{ player.prenom }}">
                                                            <img src="{{ asset('uploads/' ~ player.photo) }}" alt="player-image">
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                        {% endif %}
                                    {% endfor %}




                                    <div class="arrow-wrap">
                                        <!-- Controls -->
                                        <a class="nav-arrow left-arrow" href="#slider" role="button" data-slide="prev">
                                            <i class="fa fa-caret-left" aria-hidden="true"></i>
                                            <span class="sr-only">Previous</span>
                                        </a>
                                        <a class="nav-arrow right-arrow" href="#slider" role="button" data-slide="next">
                                            <i class="fa fa-caret-right" aria-hidden="true"></i>
                                            <span class="sr-only">Next</span>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-5">
                    <h4 class = "hockey-stats-h4">Best players</h4>
                    <div class="team-best-player">
                        <div class="">
                            <ul class="player-filters" role="tablist">
                                <li class="active">
                                    <a href="#goals" role="tab" data-toggle="tab">goals</a>
                                </li>
                                <li>
                                    <a href="#assist" role="tab" data-toggle="tab">assist</a>
                                </li>
                            </ul>
                        </div>
                        <div class="tab-content">
                            <div class="best-players-list tab-pane active" id="goals">

                                {% set rows = 0 %}
                                {% for player in j  %}
                                    {% set rows = rows +1 %}
                                    {% if rows < 7 %}
                                        <a href="{{ path('detailJ', { 'cin': player.cin }) }}" class="item">
                                            <span class="number">{{player.numt}}</span>
                                            <span>{{player.nom }} {{ player.prenom }}</span>
                                            <span class="achievement">{{player.nbm}}</span>
                                        </a>
                                    {% endif %}
                                {% endfor %}
                            </div>
                            <div class="best-players-list tab-pane" id="assist">
                                {% set rows = 0 %}
                                {% for player in x  %}
                                    {% set rows = rows +1 %}
                                    {% if rows < 7 %}
                                        <a href="{{ path('detailJ', { 'cin': player.cin }) }}" class="item">
                                            <span class="number">{{player.numt}}</span>
                                            <span>{{player.nom }} {{ player.prenom }}</span>
                                            <span class="achievement">{{player.nba}}</span>
                                        </a>
                                    {% endif %}
                                {% endfor %}


                            </div>
                        </div>


                    </div>
                </div>
            </div>
        </div>
    </section>



    <!--MAIN PLAYERS STAT END-->

<!--SUCCESS STORY BEGIN-->
