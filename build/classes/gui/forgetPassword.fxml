<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="ap" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ForgetPasswordController">
   <children>
      <Label layoutX="100.0" layoutY="59.0" text="Forget password">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Label layoutX="146.0" layoutY="167.0" text="Email" />
      <TextField fx:id="email" layoutX="224.0" layoutY="163.0" prefHeight="26.0" prefWidth="276.0" />
      <Button layoutX="410.0" layoutY="207.0" mnemonicParsing="false" onAction="#send" prefHeight="26.0" prefWidth="78.0" text="Send" />
      <AnchorPane fx:id="apCode" layoutX="100.0" layoutY="247.0" prefHeight="227.0" prefWidth="490.0">
         <children>
            <Label layoutX="49.0" layoutY="62.0" text="Code" />
            <TextField fx:id="code" layoutX="127.0" layoutY="58.0" prefHeight="26.0" prefWidth="188.0" />
            <Button layoutX="182.0" layoutY="159.0" mnemonicParsing="false" onAction="#confirme" prefHeight="26.0" prefWidth="78.0" text="Confim" />
            <Label layoutX="24.0" layoutY="118.0" text="New password" />
            <Label layoutX="127.0" layoutY="31.0" text="Check your e-mail to get code..." />
            <PasswordField fx:id="password" layoutX="127.0" layoutY="114.0" prefHeight="26.0" prefWidth="188.0" />
         </children>
      </AnchorPane>
      <Label layoutX="224.0" layoutY="137.0" text="Please enter your e-mail..." />
      <Button layoutX="26.0" layoutY="533.0" mnemonicParsing="false" onAction="#back" prefHeight="26.0" prefWidth="119.0" text="Back" />
      <ImageView fx:id="rollback" fitHeight="26.0" fitWidth="28.0" layoutX="12.0" layoutY="14.0" onMouseClicked="#rollback" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
